simcv_prog=$AUTOCONF
simcv_minversion=2.50
simcv_grepline="Autoconf"
simcv_progargs="--version"
simcv_prog_var=""
simcv_alt_prog=""

# First, make sure that $simcv_prog is non-empty. If the program wasn't found at
# all, $simcv_prog will be empty.

if ( let "0 == 0" ) 2>/dev/null && test -n "$simcv_prog"; then

	echo "$as_me:$LINENO: checking version of $simcv_prog" >&5
echo $ECHO_N "checking version of $simcv_prog... $ECHO_C" >&6

	if test ! -x "$simcv_prog"; then

	warnings=yes
	echo "*Warning*: "could not find $simcv_prog to extract version"" >&7
	{ echo "$as_me:$LINENO: WARNING: \"could not find $simcv_prog to extract version\"" >&5
echo "$as_me: WARNING: \"could not find $simcv_prog to extract version\"" >&2;}

	fi

	if test -n "$simcv_grepline"; then
		simcv_version_line=`$simcv_prog $simcv_progargs 2>&1 | grep "$simcv_grepline"`
	else
		simcv_version_line=`$simcv_prog $simcv_progargs`
	fi

	simcv_major=`echo $simcv_version_line | sed -e 's/^[^0-9]*\([0-9][0-9]*\).*/\1/'`
	simcv_minor=`echo $simcv_version_line | sed -e 's/^[^0-9]*[0-9][0-9]*\.\([0-9][0-9]*\).*/\1/'`

	simcv_rq_major=`echo $simcv_minversion | sed -e 's/^[^0-9]*\([0-9][0-9]*\).*/\1/'`
	simcv_rq_minor=`echo $simcv_minversion | sed -e 's/^[^0-9]*[0-9][0-9]*\.\([0-9][0-9]*\).*/\1/'`

        simcv_warning=""

	if test -z "$simcv_major" || test -z "$simcv_minor"; then
		simcv_warning="could not find version of $simcv_prog"
	elif test -z "$simcv_rq_major" || test -z "$simcv_rq_minor"; then
		simcv_warning="could not parse minimum version of $simcv_prog"
	elif let "$simcv_major > $simcv_rq_major" ; then
		echo "$as_me:$LINENO: result: ok ($simcv_major.$simcv_minor)" >&5
echo "${ECHO_T}ok ($simcv_major.$simcv_minor)" >&6
	elif let "$simcv_major == $simcv_rq_major && $simcv_minor >= $simcv_rq_minor" ; then
		echo "$as_me:$LINENO: result: ok ($simcv_major.$simcv_minor)" >&5
echo "${ECHO_T}ok ($simcv_major.$simcv_minor)" >&6
	else
		simcv_warning="$simcv_prog is too old ($simcv_major.$simcv_minor), need at least $simcv_rq_major.$simcv_rq_minor"
	fi

	if test -n "$simcv_warning"; then
		if test -n "$simcv_alt_prog" -a -n "$simcv_prog_var"; then

	warnings=yes
	echo "*Warning*: "Using $simcv_alt_prog as PYTHON due to problem with $simcv_prog: $simcv_warning"" >&7
	{ echo "$as_me:$LINENO: WARNING: \"Using $simcv_alt_prog as PYTHON due to problem with $simcv_prog: $simcv_warning\"" >&5
echo "$as_me: WARNING: \"Using $simcv_alt_prog as PYTHON due to problem with $simcv_prog: $simcv_warning\"" >&2;}

			eval $simcv_prog_var=$simcv_alt_prog
		else

	warnings=yes
	echo "*Warning*: "$simcv_warning"" >&7
	{ echo "$as_me:$LINENO: WARNING: \"$simcv_warning\"" >&5
echo "$as_me: WARNING: \"$simcv_warning\"" >&2;}

		fi
	fi

fi



